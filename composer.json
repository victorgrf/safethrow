{
    "name": "victorgrf/safethrow",
    "description": "Lançamento e tratamento de exceções padronizado para aplicações Laravel.",
    "type": "library",
    "require-dev": {
        "phpunit/phpunit": "^11.3",
        "orchestra/testbench": "^9.5"
    },
    "autoload": {
        "psr-4": {
            "SafeThrow\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "SafeThrow\\Tests\\": "tests/"
        }
    },
    "authors": [
        {
            "name": "victorferreira",
            "email": "victorferreira@crea-df.org.br"
        }
    ],
    "require": {},
    "minimum-stability": "stable",
    "extra": {
        "laravel": {
            "providers": [
                "SafeThrow\\SafeThrowServiceProvider"
            ]
        }
    },
    "scripts": {
        "test": "php ./vendor/bin/phpunit",
        "test:filter": "php ./vendor/bin/phpunit --filter"
    }
}
